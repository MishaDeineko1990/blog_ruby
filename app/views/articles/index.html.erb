<h1>Articles#index</h1>
<p>Find me in app/views/articles/index.html.erb</p>


    <H1>List articles</H1>

    <% @articles.each do |article|%>
        <br>
        <h3> <%= article.title %></h3>
        <p> <%= article.text %></p>
        <p>
            <%= link_to "Show", article_path(article)%> | 
            <%= link_to "Edit", edit_article_path(article)%>
        </p>
        <hr>
    <% end %>
